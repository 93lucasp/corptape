{
  "name": "corptape",
  "description": "",
  "main": "gulpfile.js",
  "authors": [
    "Luca Spezzano"
  ],
  "license": "ISC",
  "homepage": "https://github.com/93lucasp/corptape",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "components-font-awesome": "^4.7.0",
    "animate.css": "^3.5.2",
    "owl.carousel": "^2.2.1",
    "bootstrap": "v4.0.0-alpha.6",
    "wow": "wowjs#^1.1.2",
    "barba.js": "luruke/barba.js#^1.0.0"
  },
  "overrides": {
    "jquery": {
      "main": [
        "./dist/jquery.min.js"
      ]
    },
    "jquery_ui": {
      "main": [
        "./jquery-ui.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "./dist/js/bootstrap.min.js",
        "./scss/**/*.scss"
      ]
    },
    "tether": {
      "main": [
        "./dist/js/tether.min.js"
      ]
    },
    "animate.css": {
      "main": [
        "./animate.min.css"
      ]
    },
    "components-font-awesome": {
      "main": [
        "./fonts/**/*",
        "./css/font-awesome.css"
      ]
    },
    "wow": {
      "main": [
        "./dist/wow.min.js"
      ]
    },
    "jquery-mask-plugin": {
      "main": [
        "./dist/jquery.mask.min.js"
      ]
    },
    "chart.js": {
      "main": [
        "./dist/chart.min.js"
      ]
    },
    "owl.carousel": {
      "main": [
        "./dist/owl.carousel.min.js",
        "./dist/assets/owl.carousel.min.css",
        "./dist/assets/owl.theme.default.min.css"
      ]
    },
    "barba.js": {
      "main": [
        "./dist/barba.min.js"
      ]
    }
  }
}
